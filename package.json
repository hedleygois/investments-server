{
  "name": "investments-server",
  "version": "0.0.0",
  "main": "App.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "start": "node ./dist --env=production",
    "start:dev": "nodemon",
    "test": "cross-env NODE_ENV=test jest -c jest.config.js",
    "test:watch": "jest -c jest.config.js --watchAll",
    "lint": "./node_modules/eslint/bin/eslint.js --fix -c .eslintrc.js --ext .ts src"
  },
  "engines": {
    "node": "12.x"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.0",
    "sequelize": "^6.3.3",
    "tslib": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.11",
    "@types/jest": "^26.0.4",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.1",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.0.22",
    "@types/node-fetch": "^2.5.7",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsdoc": "^29.1.3",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-promise": "^4.2.1",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jest": "^26.1.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  }
}
